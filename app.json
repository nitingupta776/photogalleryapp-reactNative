{
  "expo": {
    "name": "FirstReactNativeApp",
    "slug": "firstreactnativeapp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Photo Gallery App with touch, motion, gesture, and speech API sensors",
    "githubUrl": "https://github.com/nitingupta776/PhotoGalleryReactNativeApp"
  }
}
